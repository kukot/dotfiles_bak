#THIS MUST BE AT THE END OF THE FILE FOR SDKMAN TO WORK!!!
export SDKMAN_DIR="$HOME/.sdkman"
[[ -s "$HOME/.sdkman/bin/sdkman-init.sh" ]] && source "$HOME/.sdkman/bin/sdkman-init.sh"

#custom java11_home for using with kotlin-language-server
export JAVA11_HOME=$HOME/.sdkman/candidates/java/11-amzn
export JAVA21_HOME=$HOME/.sdkman/candidates/java/21.0.2-amzn
export JAVA17_HOME=$HOME/.sdkman/candidates/java/17.0.10-amzn
export ESSDOCS_ENABLED=false

export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"  # This loads nvm
[ -s "$NVM_DIR/bash_completion" ] && \. "$NVM_DIR/bash_completion"  # This loads nvm bash_completion

export CONTOUR_SNAPSHOT_HOME=$HOME/contour-workspace/snapshot


export JAVA_HOME=$JAVA21_HOME
